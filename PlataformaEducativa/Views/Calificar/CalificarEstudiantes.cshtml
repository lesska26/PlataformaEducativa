@model List<CursoParticipante>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

<div>
    <table class="table table-sm">
        <thead>
            <tr>
                <th scope="col-1">
                    #
                </th>
                <th scope="col-1">
                    Nombre
                </th>
                <th scope="col-1">
                    Califcar Nota
                </th>
            </tr>
        </thead>
        @{
            int f = 1;
        }
        @foreach (var i in Model)
        {
            <tr >
                <th>
                    #@f
                </th>
                <td>
                    @i.Estudiantes.Nombre
                </td>
                <td name="info">
                    
                    <input name="@i.IniciarCursoId" type="number" id="@i.EstudiantesId">
                </td>
            </tr>
            f++;
        }
    </table>
    <div>
        <button  class="btn btn-outline-info"  id="Calificar">Calificar</button>
    </div>
</div>
<script src="~/js/NotaEstudiantes.js"></script>